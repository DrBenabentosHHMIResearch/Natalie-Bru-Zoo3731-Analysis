#ATMI Pre Bargraph 
ggplot(l, aes(x=factor(variable), y=value)) +
  geom_bar(position="dodge", stat="identity",aes(fill=factor(variable)))+
             facet_grid(Term ~.)+
theme(legend.position="none")+ #this removes the legend
  ylab("ATMI Mean % Score") + xlab("")+
  scale_fill_brewer(direction=-1, palette="Paired")+
  scale_x_discrete(labels=c("Enjoyment","Motivation","Self-Confidence","Value"))


@
\normalsize
{\begin{itemize}
   \item  The semesters are indicated by their 4 digit codes; 1158 and 1161 for Fall '15 and Spring '16, respectively. 
  \item Bars indicate mean scores of ATMI subscales:
  \begin{itemize}
    \item Enjoyment (5 items, 0-25 scale)
    \item Motivation (4 items, 0-20 scale)
    \item Self confidence (5 items, 0-25 scale)
    \item Value (5 items, 0-25 scale)
  \end{itemize}
\end{itemize}}
\end{flushleft}

